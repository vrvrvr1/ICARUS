<head>
    <link rel="stylesheet" href="css/footer.css">
</head>



<!-- Footer -->
<footer class="footer">
    <div class="footer-content">
        <div class="footer-section">
            <h3>Quick Links</h3>
            <ul>
                <li><a href="#faq">FAQ</a></li>
                <li><a href="/help">Help</a></li>
                <li><a href="#contact">Contact Us</a></li>
                <li><a href="#return-policy">Return Policy</a></li>
                <li><a href="#warranty-policy">Warranty Policy</a></li>
                <li><a href="/aboutus">About Us</a></li>
            </ul>
        </div>
        <div class="footer-section">
            <h3>Contact Us</h3>
            <p>Email: <%= (typeof cmsFooter !== 'undefined' && cmsFooter && cmsFooter.contact_email) ? cmsFooter.contact_email : 'support@icaruswear.com' %></p>
            <p>Phone: <%= (typeof cmsFooter !== 'undefined' && cmsFooter && cmsFooter.phone) ? cmsFooter.phone : '(555) 123-4567' %></p>
            <p>Address: <%= (typeof cmsFooter !== 'undefined' && cmsFooter && cmsFooter.address) ? cmsFooter.address : '123 Fitness Lane, Fit City, FC 12345' %></p>
        </div>
        <div class="footer-section">
            <h3>Follow Us</h3>
            <div class="social-icons">
              <a href="#" class="social-icon"><i class="bi bi-twitter-x"></i></a>
              <a href="#" class="social-icon"><i class="bi bi-instagram"></i></a>
              <a href="#" class="social-icon"><i class="bi bi-facebook"></i></a>
            </div>
        </div>
    </div>
    <div class="footer-bottom">
        <p>&copy; <%= (typeof cmsFooter !== 'undefined' && cmsFooter && cmsFooter.copyright_text) ? cmsFooter.copyright_text : (new Date().getFullYear() + ' Icarus Wear. All rights reserved.') %></p>
    </div>
</footer>